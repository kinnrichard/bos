{
	"name": "bos-frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "DEBUG=bos:* vite dev",
		"dev:quiet": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"test:mocked": "TEST_STRATEGY=mocked playwright test",
		"test:real-db": "TEST_STRATEGY=real_db USE_REAL_DB=true playwright test",
		"test:unit": "playwright test --project=unit",
		"test:integration": "playwright test --project=integration",
		"test:e2e": "playwright test --project=e2e",
		"test:debug": "DEBUG=true playwright test --debug",
		"test:headed": "playwright test --headed",
		"test:report": "playwright show-report",
		"build:test": "vite build --mode test --config vite.config.test.ts",
		"preview:test": "vite preview --mode test --config vite.config.test.ts",
		"sync": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"format": "prettier --check ."
	},
	"devDependencies": {
		"@eslint/js": "^9.30.0",
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.22.2",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@types/debug": "^4.1.12",
		"@types/eslint": "^9.0.0",
		"@types/qs": "^6.14.0",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^16.2.0",
		"jsdom": "^26.1.0",
		"postcss": "^8.4.47",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"svelte": "^5.35.4",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.16",
		"typescript": "^5.0.0",
		"vite": "^6.0.0",
		"vitest": "^3.2.4"
	},
	"type": "module",
	"dependencies": {
		"@melt-ui/svelte": "^0.86.6",
		"@tanstack/svelte-query": "^5.62.9",
		"debug": "^4.4.1",
		"qs": "^6.14.0"
	}
}
